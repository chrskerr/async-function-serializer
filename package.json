{
	"name": "async-function-serializer",
	"version": "1.4.1",
	"description": "A helper which produces serial async functions without losing return values",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && uglifyjs dist/index.js -m -c -o dist/index.js",
		"start": "npm run build && node dist/index.js",
		"test": "npm run build && jest",
		"deploy": "npm run test && npm publish"
	},
	"author": "Chris Kerr",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.11",
		"@typescript-eslint/eslint-plugin": "^5.5.0",
		"@typescript-eslint/parser": "^5.5.0",
		"eslint": "^8.3.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"jest": "^27.4.3",
		"ts-jest": "^27.0.7",
		"typescript": "^4.5.4",
		"uglify-js": "^3.14.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chrskerr/async-function-serializer"
	},
	"keywords": [
		"typescript",
		"ts",
		"serial",
		"async",
		"function",
		"serialiser"
	],
	"sideEffects": false
}
