{
	"name": "async-function-serializer",
	"version": "1.4.3",
	"description": "A helper which produces serial async functions without losing return values",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc && uglifyjs dist/index.js -m -c -o dist/index.js",
		"start": "npm run build && node dist/index.js",
		"test": "jest",
		"deploy": "npm run build && npm run test && npm publish"
	},
	"author": "Chris Kerr",
	"license": "GPL-3.0",
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.10.0",
		"eslint-plugin-tsdoc": "^0.2.14",
		"jest": "^27.5.1",
		"ts-jest": "^27.1.3",
		"typescript": "^4.5.5",
		"uglify-js": "^3.15.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/chrskerr/async-function-serializer"
	},
	"keywords": [
		"typescript",
		"ts",
		"serial",
		"async",
		"function",
		"serialiser"
	],
	"sideEffects": false
}
